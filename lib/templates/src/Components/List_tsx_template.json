{
	"type": "Component",
	"placeholder": "___componentName___",
	"Surffix": "List",
	"content": [
		"import * as React from 'react';",
		"\nimport { Link } from 'react-router-dom';",
		"\nimport ReactTable from 'react-table';",
		"\nimport 'react-table/react-table.css';",
		"\nimport { ___componentName___ModelType } from '../Types/___componentName___ModelType';",
		"\n",
		"\nconst ___componentName___List = (props: ___componentName___ListProps) => {",
		"\n\tconst columns = [",
		"\n\t\t{",
		"\n\t\t\tHeader: 'Id',",
		"\n\t\t\taccessor: 'id'",
		"\n\t\t},",
		"\n\t\t{",
		"\n\t\t\tCell: (d: any) => <span className=\"___componentName___Name\">{d.value}</span>,",
		"\n\t\t\tHeader: 'Name',",
		"\n\t\t\taccessor: '___componentName___Name'",
		"\n\t\t},",
		"\n\t\t{",
		"\n\t\t\tCell: (d: any) => (",
		"\n\t\t\t\t<Link",
		"\n\t\t\t\t\tto={`/___componentName___/$\\{ d.original.id }`}",
		"\n\t\t\t\t\tclassName=\"btn btn-outline-success btn-sm mx-1\"",
		"\n\t\t\t\t>",
		"\n\t\t\t\t\t{' '}",
		"\n\t\t\t\t\tDetails",
		"\n\t\t\t\t</Link>",
		"\n\t\t\t),",
		"\n\t\t\tHeader: '',",
		"\n\t\t\tfilterable: false,",
		"\n\t\t\tid: 'btn_details',",
		"\n\t\t\tmaxWidth: 75",
		"\n\t\t},",
		"\n\t\t{",
		"\n\t\t\tCell: (d: any) => (",
		"\n\t\t\t\t<input",
		"\n\t\t\t\t\ttype=\"button\"",
		"\n\t\t\t\t\tclassName=\"btn btn-outline-danger btn-sm mx-1\"",
		"\n\t\t\t\t\tonClick={() => {",
		"\n\t\t\t\t\t\tprops.on___componentName___Delete(d.original);",
		"\n\t\t\t\t\t}}",
		"\n\t\t\t\t\tvalue=\"Delete\"",
		"\n\t\t\t\t/>",
		"\n\t\t\t),",
		"\n\t\t\tHeader: '',",
		"\n\t\t\tfilterable: false,",
		"\n\t\t\tid: 'btn_delete',",
		"\n\t\t\tmaxWidth: 75",
		"\n\t\t}",
		"\n\t];",
		"\n",
		"\n\treturn (",
		"\n\t\t<div>",
		"\n\t\t\t{/* List of ___componentName___s */}",
		"\n\t\t\t{console.log(`props.___componentName___Array:`, props)}",
		"\n\t\t\t<ReactTable",
		"\n\t\t\t\tdata={props.___componentName___Array ? props.___componentName___Array : undefined}",
		"\n\t\t\t\tcolumns={columns}",
		"\n\t\t\t\tdefaultPageSize={10}",
		"\n\t\t\t\tminRows={3}",
		"\n\t\t\t\tfilterable",
		"\n\t\t\t\tdefaultFilterMethod={(filter, row) =>",
		"\n\t\t\t\t\tString(row[filter.id]).includes(filter.value)",
		"\n\t\t\t\t}",
		"\n\t\t\t/>",
		"\n\t\t</div>",
		"\n\t);",
		"\n};",
		"\n",
		"\ninterface ___componentName___ListProps {",
		"\n\t___componentName___Array: ___componentName___ModelType[];",
		"\n\ton___componentName___Delete: (___componentName___: ___componentName___ModelType) => void;",
		"\n}",
		"\n",
		"\nexport default ___componentName___List;"
	]
}
